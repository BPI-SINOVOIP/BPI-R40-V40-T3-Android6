<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.allwinner.carvideo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="17" />
	 <uses-permission android:name="android.permission.CAMERA" />
	 <uses-permission android:name="android.permission.RECORD_AUDIO" />
	 <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	 
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
         >
        <activity
            android:name="com.allwinner.carvideo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name="com.allwinner.carvideo.VideoService" />
        <activity
            android:name="com.allwinner.carvideo.videoplay.VideoPlayActivity"/>
        <activity
            android:name="com.allwinner.carvideo.VideoSettingActivity" 
            ></activity>
            
    </application>

</manifest>